#Default values

export ARTIFACTORY_HOME=/home/{{ artifactory_user }}/artifactory-{{ artifactory_version}}
export ARTFICATORY_USER={{ artifactory_user }}

export TOMCAT_HOME=$ARTIFACTORY_HOME/tomcat
export ARTIFACTORY_PID=$ARTIFACTORY_HOME/run/artifactory.pid

export JAVA_OPTIONS="-server -Xms256m -Xmx2g -Xss256k -XX:PermSize=128m -XX:MaxPermSize=128m -XX:+UseG1GC"
